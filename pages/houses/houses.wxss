.houses-page {
  padding: 210rpx 0 80px 0;
}

.search-box {
  position: fixed;
  top: 0;
  left: 0;
  z-index: 999;
  background: #F7F8F8;
  height: 110rpx;
  width: 100%;
  background: F7F8F8;
  padding: 20rpx 32rpx;
}

.search-box .inner-box {
  display: flex;
  align-items: center;
  justify-content: center;
  width: 100%;
  height: 100%;
  border-radius: 8rpx;
  background: #fff;
  font-size: 26rpx;
  color: #B8BFC3;
}
.search-box .inner-box .search-icon {
  margin-right: 8rpx;
  width: 28rpx;
  height: 28rpx;
}
.houses-page .tab-box {
  position: fixed;
  top: 110rpx;
  left: 0;
  z-index: 999;
  width: 100%;
  display: flex;
  justify-content: space-between;
  align-items: center;
  background: #fff;
  height: 100rpx;
  box-shadow: 0 2rpx 4rpx 0 #eee;
}
.content-box {
  padding: 24rpx;
}
.house-index-inner-box {
  box-shadow: 0 0 16rpx 0 rgba(184,191,195,0.25);
  border-radius: 16rpx;
  margin-bottom: 24rpx;
}
.tab-item-box {
  width: 25%;
  display: flex;
  justify-content: center;
  align-items: center;
  color: #727483;
  font-size: 28rpx;
}
.tab-item-box .txt1 {
  font-weight: 500;
}

.tab-item-box.active {
  color: #F6515D;
}

