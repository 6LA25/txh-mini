.pop-filter-box {
  position: fixed;
  top: 210rpx;
  left: 0;
  bottom: 0;
  width: 100vw;
  z-index: 999;
}

.mask-box {
  position: absolute;
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0.7);
  z-index: -1
}
.area-parent-filters-box {
  background: #fff;
  padding-bottom: 20rpx;
}
.area-parent-filters-box .btns-box {
  padding: 0 32rpx;
}
.area-filters-box {
  display: flex;
  flex-wrap: wrap;
  background: #fff;
  color: #333333;
  font-size: 28rpx;
  padding: 20rpx 32rpx 0 20rpx;
}

.area-filters-box .area-item {
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 24rpx;
  color: #727483;
  background: #F7F8F8;
  width: 140rpx;
  height: 58rpx;
  border-radius: 4rpx;
  margin-bottom: 20rpx;
  margin-right: 42rpx;
}
.area-filters-box .area-item:nth-child(4n+0) {
  margin-right: 0;
}

.house-type-selecte-box {
  position: relative;
  background: #fff;
  padding: 10rpx 32rpx;
  font-size: 28rpx;
  color: #333333;
}

.checkbox-mask-box {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  z-index: 2;
}

.checkbox-item-box {
  display: flex;
  align-items: center;
  justify-content: space-between;
  height: 80rpx;
}

.checkbox-item-box checkbox .wx-checkbox-input {
  width: 32rpx;
  height: 32rpx;
  border-radius: 2rpx;
}

.checkbox-item-box checkbox .wx-checkbox-input.wx-checkbox-input-checked {
  border-color: #F6515D;
}

.checkbox-item-box checkbox .wx-checkbox-input.wx-checkbox-input-disabled {
  background: #fff;
  color: #F6515D;
}

.more-filter-box {
  padding: 32rpx;
  background: #fff;
}

.more-filter-box .title {
  font-size: 28rpx;
  color: #333333;
  margin-bottom: 8rpx;
}

.more-filter-box .tag-box {
  display: flex;
  flex-wrap: wrap;
  margin-bottom: 10rpx;
}

.more-filter-box .tag-box .tag-item {
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 24rpx;
  color: #727483;
  background: #F7F8F8;
  width: 140rpx;
  height: 58rpx;
  border-radius: 8rpx;
  margin: 0 42rpx 16rpx 0
}

.more-filter-box .tag-box .tag-item:nth-child(4n+0) {
  margin-right: 0;
}

.more-filter-box .tag-box .tag-item.active, .area-filters-box .area-item.active {
  /* background: #fceae5;
  color: #F6515D; */
  position: relative;
  background: #fff;
  border: 1px solid #E32835;
  border-radius: 8rpx;
  color: #F6515D;
}

.more-filter-box .tag-box .tag-item.active:after, .area-filters-box .area-item.active:after {
  content: '';
  position: absolute;
  bottom: 0;
  right: 0;
  display: block;
  width: 42rpx;
  height: 38rpx;
  background-image: url("data:image/png;base64,");
  background-size: cover;
}

.btns-box {
  display: flex;
  justify-content: space-between;
  margin-top: 30rpx;
}

.btns-box .btn-item {
  display: flex;
  align-items: center;
  justify-content: center;
  width: 45%;
  height: 80rpx;
  border-radius: 8rpx;
  font-size: 30rpx;
}

.btns-box .btn-item.reset {
  color: #E32835;
  background: #F7F8F8;
}

.btns-box .btn-item.confirm {
  color: #FFFFFF;
  background: #E32835;
}

.triangle {
  width: 16rpx;
  height: 8rpx;
  margin-left: 6rpx;
}

.price-triangle-box {
  display: flex;
  flex-direction: column;
}

.price-triangle-box .top {
  margin-bottom: 4rpx;
}

.no-house-box {
  margin-top: 100rpx;
}
.price-tab-box {
  display: flex;
  justify-content: space-between;
  color: #727483;
  font-size: 28rpx;
  margin-bottom: 30rpx;
}
.price-tab-box .price-tab-item {
  position: relative;
  flex: 1;
  display: flex;
  justify-content: center;
  align-items: center;
  height: 60rpx;
}
.price-tab-box .price-tab-item.active {
  color: #F6515D;
}
.price-tab-box .price-tab-item .border-bottom-box {
  position: absolute;
  bottom: 0;
  left: 50%;
  transform: translate(-50%, 0);
  width: 60rpx;
  height: 6rpx;
  background: #F6515D;
}